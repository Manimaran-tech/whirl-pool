{
    "$schema": "https://openapi.vercel.sh/vercel.json",
    "buildCommand": "npm install --prefix ./whirlpool-dashboard && npm run build --prefix ./whirlpool-dashboard",
    "outputDirectory": "whirlpool-dashboard/dist",
    "installCommand": "npm install --prefix ./whirlpool-dashboard",
    "framework": null,
    "rewrites": [
        {
            "source": "/(.*)",
            "destination": "/"
        }
    ]
}